<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="false"
    android:textAlignment="center"
    tools:context=".ui.view.fragment.PrincipalFragment">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="70dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="26dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_menu"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_margin="10dp"
            android:clickable="true"
            android:src="@drawable/ic_baseline_menu_24" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="24dp"
            android:layout_height="44dp"
            android:layout_weight="1" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_carrito"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_margin="10dp"
            android:clickable="true"
            android:src="@drawable/ic_baseline_add_shopping_cart_24" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="448dp"
        android:layout_height="49dp"
        android:layout_marginTop="96dp"
        android:orientation="vertical"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="343dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="60dp"
            android:layout_marginEnd="42dp"
            android:text="¿ Que vas a celebrar ?"
            android:textAlignment="viewStart"
            android:textSize="34sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="471dp"
        android:layout_height="137dp"
        android:layout_marginTop="34dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="110dp"
            android:layout_height="89dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="37dp"
            android:layout_marginBottom="30dp"
            android:src="@drawable/cena" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="54dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="56dp"
            android:text="Cena Romantica"
            android:textAlignment="center"
            android:textSize="20sp"
            tools:ignore="MissingConstraints" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_cena"
            android:layout_width="57dp"
            android:layout_height="58dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="37dp"
            android:layout_marginBottom="28dp"
            android:clickable="true"
            android:src="@drawable/ic_baseline_arrow_circle_right_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="458dp"
        android:layout_height="122dp"
        android:layout_marginBottom="1dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="110dp"
            android:layout_height="88dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="39dp"
            android:layout_marginBottom="16dp"
            android:src="@drawable/cumpleanos" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="53dp"
            android:layout_marginEnd="49dp"
            android:layout_marginBottom="44dp"
            android:text="Cumpleaños"
            android:textSize="20sp" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_cumple"
            android:layout_width="57dp"
            android:layout_height="58dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="37dp"
            android:layout_marginBottom="28dp"
            android:clickable="true"
            android:src="@drawable/ic_baseline_arrow_circle_right_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="466dp"
        android:layout_height="121dp"
        android:layout_marginBottom="2dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="109dp"
            android:layout_height="92dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="44dp"
            android:layout_marginBottom="11dp"
            android:src="@drawable/graduacion"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="43dp"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="52dp"
            android:text="Graduación"
            android:textSize="20sp" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_graduacion"
            android:layout_width="57dp"
            android:layout_height="58dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="37dp"
            android:layout_marginBottom="28dp"
            android:clickable="true"
            android:src="@drawable/ic_baseline_arrow_circle_right_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="469dp"
        android:layout_height="110dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="60dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="110dp"
            android:layout_height="89dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="46dp"
            android:layout_marginBottom="7dp"
            android:src="@drawable/mas" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="46dp"
            android:layout_height="31dp"
            android:layout_marginTop="42dp"
            android:layout_marginEnd="108dp"
            android:layout_marginBottom="41dp"
            android:text="Otro"
            android:textSize="20sp" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_otro"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="28dp"
            android:clickable="true"
            android:src="@drawable/ic_baseline_arrow_circle_right_24" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

      